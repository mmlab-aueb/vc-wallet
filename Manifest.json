{
	"manifest_version": 2,
	"name": "Credential Handler",
	"version": "0.1",
	"browser_action": {
		"default_icon": "assets/zerotrustvc_icon.png",
		"default_popup": "html/popup.html",
		"default_title": "Verifiable Credential Handler"
	},
	"background": {
		"page": "html/background.html"
	 },
	"permissions": [
    	"storage",
    	"webRequest",
		"webRequestBlocking",
		"<all_urls>"
  	]
}